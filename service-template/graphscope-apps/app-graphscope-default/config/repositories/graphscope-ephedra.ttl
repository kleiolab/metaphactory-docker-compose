@prefix ephedra: <http://www.metaphacts.com/ontologies/platform/ephedra#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rep: <http://www.openrdf.org/config/repository#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

[] a rep:Repository;
	rep:repositoryID "graphscope-ephedra";
	rep:repositoryImpl [
		ephedra:defaultMember "default";
		ephedra:serviceMember [
			ephedra:delegateRepositoryID "graphscope-lookup";
			ephedra:serviceReference <http://www.metaphacts.com/ontologies/platform/repository/federation#graphscope-lookup>
		];
		rep:repositoryType "metaphacts:EphedraRepository"
	];
	rdfs:label "Ephedra federation for GraphScope" .